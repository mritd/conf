{
    "init": true,
    "data-root": "/data/docker",
    "exec-opts": ["native.cgroupdriver=systemd"],
    "live-restore": true,
    "log-driver": "json-file",
    "log-level": "info",
    "log-opts": {
        "cache-max-file": "5",
        "cache-max-size": "20m",
        "cache-compress": "true",
        "max-file": "5",
        "max-size": "10m"
    }
}
